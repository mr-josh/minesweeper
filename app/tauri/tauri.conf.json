{
  "$schema": "../www/node_modules/@tauri-apps/cli/schema.json",
  "build": {
    "beforeBuildCommand": {
      "cwd": "../www",
      "script": "npm run build"
    },
    "beforeDevCommand": {
      "cwd": "../www",
      "script": "npm run dev"
    },
    "devPath": "http://localhost:5173",
    "distDir": "../www/dist"
  },
  "package": {
    "productName": "minesweeper",
    "version": "0.1.0"
  },
  "tauri": {
    "allowlist": {
      "all": true
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.mr-josh.minesweeper",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 700,
        "resizable": true,
        "title": "Minesweeper",
        "width": 900,
        "decorations": false,
        "minWidth": 240,
        "minHeight": 240
      }
    ]
  }
}
